{
    "name": "spawaskar/cora",
    "description": "Commingled Remains Analytics (CoRA)",
    "keywords": [
        "commingled human remains", "human remains", "skeletal", "bones", "forensic", "anthropology",
        "biological profile", "dna", "haplogroup", "pair matching", "articulations", "taphonomy",
        "data science", "data analytics", "data visualization",
        "anomaly", "pathology", "trauma",
        "osteology", "osteosort", "z-transform", "graph clustering"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-cors": "^1.0",
        "bepsvpt/secure-headers": "^5.0",
        "beyondcode/laravel-self-diagnosis": "^1.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "devdojo/chatter": "0.2.*",
        "doctrine/dbal": "^2.10",
        "felixkiss/uniquewith-validator": "^3.3",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.0",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^9.0",
        "pragmarx/google2fa-laravel": "^1.3",
        "predis/predis": "^1.1",
        "tcg/voyager": "1.3.*",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/dusk-dashboard": "^2.0",
        "filp/whoops": "~2.7",
        "fzaninotto/faker": "~1.9",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "1.3.*",
        "phpunit/phpunit": "~8.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "ln -sr storage/app/public public/storage"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
